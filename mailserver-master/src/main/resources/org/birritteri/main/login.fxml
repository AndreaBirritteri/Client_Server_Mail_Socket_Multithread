        <Label alignment="CENTER" layoutX="112.0" layoutY="200.0" prefHeight="26.0" prefWidth="102.0" text="Username:" />
        <TextField fx:id="username" layoutX="225.0" layoutY="200.0" />
        <Button layoutX="380.0" layoutY="200.0" mnemonicParsing="false" onAction="#onLoginButtonClick" prefHeight="26.0" prefWidth="85.0" text="LOGIN" />
        <Text fx:id="loginError" fill="RED" layoutX="389.0" layoutY="187.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Invalid username." visible="false" />
        <Button layoutX="473.0" layoutY="200.0" mnemonicParsing="false" onAction="#onRegisterButtonClick" prefHeight="26.0" prefWidth="85.0" text="SIGNIN" />
    </children>
</AnchorPane>
